    "sort-packages": true
  },
  "require": {
    "php": ">=5.5.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "lustmored/lockme-sdk": "^1.0.0",
    "paragonie/random_compat": "<9.99"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "license": "GPLv2",
  "authors": [
    {
